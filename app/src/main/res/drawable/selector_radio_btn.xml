<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape android:shape="rectangle">
            <solid android:color="#FFB84F" />
            <stroke android:width="1dp" android:color="#FBD86B" />
            <corners
                android:radius="10dp"/>
        </shape>
    </item>
    <item android:state_checked="false">
        <shape android:shape="rectangle">
            <solid android:color="#9F9186" />
            <stroke android:width="1dp" android:color="#9F9186" />
            <corners
                android:radius="10dp"/>
        </shape>
    </item>
</selector>
